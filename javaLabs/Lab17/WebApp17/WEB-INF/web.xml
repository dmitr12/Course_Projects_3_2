<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
 http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
    <context-param>
        <description>parameter URL1</description>
        <param-name>URL1</param-name>
        <param-value>http://localhost:8080/WebApp11/html/education.html</param-value>
    </context-param>
    <context-param>
        <description>parameter URL2</description>
        <param-name>URL2</param-name>
        <param-value>http://localhost:8080/WebApp12/GoSss</param-value>
    </context-param>
    <servlet>
        <servlet-name>Sss</servlet-name>
        <servlet-class>Sss</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Sss</servlet-name>
        <url-pattern>/Sss</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Ccc</servlet-name>
        <servlet-class>Ccc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ccc</servlet-name>
        <url-pattern>/Ccc</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>html/index.html</welcome-file>
    </welcome-file-list>
</web-app>